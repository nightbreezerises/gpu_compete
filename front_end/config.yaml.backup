target-yaml-path: ../config.yaml # 要操作的yaml文件路径（支持绝对路径和相对路径，相对路径是相对于当前.yaml文件的）
log-dir: ./logs/ # 日志存储目录
port: 29217 # 当前任务运行端口（如果冲突了自动换一个）
allow-lan: true # 允许来自局域网外部主机的访问
bind-address: 127.0.0.1 # 绑定地址
log-level: info  # 日志级别
external-controller: 0.0.0.0:23660 # 外部控制器地址
secret: "omniai315fgvr_gpu_config" # 访问密钥（用于API认证）

